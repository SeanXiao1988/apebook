<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{title}}</title>
    <link rel="stylesheet" href="{{host}}/bootstrap.min.css">
    <link rel="stylesheet" href="{{host}}/bootstrap-responsive.min.css">
    <link rel="stylesheet" href="{{host}}/default.css">
    <link rel="stylesheet" href="{{host}}/admin.css">
    <link rel="stylesheet" href="{{host}}/style-metro.css">
    <link rel="stylesheet" href="{{host}}/admin-responsive.css">
    <script src="http://cdn.bootcss.com/jquery/1.11.1/jquery.min.js"></script>
    <script src="http://cdn.bootcss.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    {{{block ("head")}}}
</head>
<body class="page-header-fixed">
{{include ("../sub/header")}}
<div class="page-container">
    {{include ("../sub/sidebar")}}
    <div class="page-content">
        <div class="container-fluid">
            <div class="row-fluid">
                <div class="span12">
                    <h3 class="page-title">{{title}}</h3>
                </div>
            </div>
        </div>
        {{{block ("body")}}}
    </div>
</div>
<div class="footer">

    <div class="footer-inner">

        2014 © jujs by 明河.

    </div>

    <div class="footer-tools">

			<span class="go-top">

			<i class="icon-angle-up"></i>

			</span>

    </div>

</div>
</body>
</html>